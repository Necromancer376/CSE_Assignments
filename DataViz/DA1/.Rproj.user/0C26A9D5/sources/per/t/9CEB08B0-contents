x <- c(5, 7, 9)
y <- c(6, 3, 4)
z <- cbind(x, y)

dim(z)
rbind(z , z)

z <- matrix(c(5, 7, 9, 3, 4), nrow=3)
z <- matrix(c(5, 7, 9, 3, 4), ncol=3)
z <- matrix(c(5, 7, 9, 3, 4), nr=3, byrow=T)
z <- matrix(c(5, 7, 9, 3, 4), nrow=3, byrow=F)
y <- matrix(c(1,3,0,9,5,-1), nrow=3, byrow=T)

y + z
y * z

x <- matrix(c(3,4,-2,6), nrow=2, byrow=T)

y%*%x

solve(x)

z[1,1]
z[c(2,3), 2]
z[,2]
